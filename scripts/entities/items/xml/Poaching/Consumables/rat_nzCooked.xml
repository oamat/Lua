<item name="rat_nzCooked" class="Consumable" category="any"> 

	<params>
		<param name="selectable" value="0"/>
		<param name="mass" value="1.0"/>
		<param name="stack_size" value="10"/>		
		<param name="display_name" value="rat Cooked"/>
		<param name="description" value="rat cooked, for give stamina !"/>
	</params>

	<geometry>
		<thirdperson name="Objects/NationZ/Consumables/rat/rat_nzCooked.cgf" material="Objects/NationZ/Consumables/rat/rat_nzCooked.mtl" />
	</geometry>

	<consumable>
		<!-- a cookable item must have cookedItem and cookedTime defined -->
		<param name="cookable" value="1"/>
		<param name="cookedItem" value="rat_nzBurned"/>
		<param name="cookedTime" value="60"/>
		<param name="foodAmount" value="300"/>
		<param name="staminaAmount" value="30"/>
		<param name="torpidityAmount" value="-80"/>
		<param name="soundConsumed" value="Play_food_soft_eat"/>
	</consumable>

	<storage>
		<locations>
			<location category="any"/>
		</locations>
	</storage>

</item>